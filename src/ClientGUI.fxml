<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane id="ChatTextField" prefHeight="554.0" prefWidth="652.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ClientGUI">
  <center>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
        <TextArea id="ChatBoardArea" fx:id="ChatArea" editable="false" prefHeight="412.0" prefWidth="552.0" text="" wrapText="true" VBox.vgrow="ALWAYS" />
        <TextField fx:id="ChatTextField" onKeyPressed="#ChatFieldTyping" prefWidth="200.0" />
      </children>
    </VBox>
  </center>
  <left>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
        <Label alignment="CENTER" contentDisplay="LEFT" prefHeight="36.0" prefWidth="100.0" text="Clients" textAlignment="LEFT">
          <font>
            <Font size="20.0" />
          </font>
        </Label>
        <ListView fx:id="ClientList" prefHeight="399.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
      </children>
    </VBox>
  </left>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Close" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Delete" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</BorderPane>
